{
    "name": "next-admin-starter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "cross-env NODE_ENV=development next dev --turbopack",
        "dev:webpack": "cross-env NODE_ENV=development next dev",
        "build": "rm -rf .next && next build",
        "start": "next start",
        "analyzer": "cross-env ANALYZE=true next build",
        "prepare": "husky install",
        "lint": "eslint src/",
        "format": "prettier --write .",
        "add:ui": "npx shadcn@latest add",
        "clean": "rimraf ./node_modules"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{css,scss,md,json}": [
            "prettier --write"
        ]
    },
    "author": "tech-zjf",
    "dependencies": {
        "@next/bundle-analyzer": "^15.0.3",
        "@next/third-parties": "^15.3.5",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tooltip": "^1.2.8",
        "axios": "^1.7.8",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "copy-to-clipboard": "^3.3.3",
        "dayjs": "^1.11.13",
        "immer": "^10.1.1",
        "js-cookie": "^3.0.5",
        "lodash": "^4.17.21",
        "next": "16.1.3",
        "proxy-agent": "^6.4.0",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "use-immer": "^0.11.0",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.2",
        "@tailwindcss/forms": "^0.5.10",
        "@types/ali-oss": "^6.16.1",
        "@types/eslint": "^9.6.1",
        "@types/js-cookie": "^3.0.6",
        "@types/lodash": "^4.17.13",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "chalk": "^4.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^9",
        "eslint-config-next": "^16.1.3",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^8.0.1",
        "lint-staged": "^10.0.0",
        "os-locale": "^5.0.0",
        "postcss": "^8",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    },
    "engines": {
        "node": ">=22.21.0"
    }
}
